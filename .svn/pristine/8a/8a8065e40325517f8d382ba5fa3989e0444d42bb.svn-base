.tipss {
  width: 720px;
  height: 56px;
  background-color: #ffffff;
  padding-left: 20px;
  line-height: 56px;
  color: #ff8d24;
  font-size: 14px;
  /*no*/
  position: fixed;
  top: 100px;
  z-index: 100;
  left: 50%;
  -webkit-transform: translateX(-50%);
  -moz-transform: translateX(-50%);
  -ms-transform: translateX(-50%);
  -o-transform: translateX(-50%);
  transform: translateX(-50%); }
  [data-dpr="2"] .tipss {
    font-size: 28px;
    /*no*/ }
  [data-dpr="3"] .tipss {
    font-size: 42px;
    /*no*/ }

.all_checkbox {
  width: 720px;
  padding: 10px 0 10px 20px;
  background-color: #f8f6f3;
  position: fixed;
  top: 156px;
  z-index: 100;
  left: 50%;
  -webkit-transform: translateX(-50%);
  -moz-transform: translateX(-50%);
  -ms-transform: translateX(-50%);
  -o-transform: translateX(-50%);
  transform: translateX(-50%); }

.input_checkbox {
  width: 20px;
  height: 20px;
  background-color: #e0e0e0;
  float: left;
  outline: 0; }

.checkbox_font {
  height: 20px;
  line-height: 20px;
  float: left;
  margin-left: 12px; }

.stayPay .main {
  width: 720px;
  margin: 0 auto;
  padding-top: 196px; }

.stayPay .shoppingList_box_outer {
  position: relative;
  margin-bottom: 17px; }

.stayPay .checkbox_box {
  position: absolute;
  top: 22px;
  left: 19px; }

.stayPay .shoppingList_box {
  padding: 0 50px;
  background-color: #ffffff; }
  .stayPay .shoppingList_box li {
    border-bottom: 1px solid #e3e3e3;
    /*no*/ }
  .stayPay .shoppingList_box li:last-child {
    border-bottom: none; }

.stayPay .item_title {
  /*height: 107px;*/
  padding: 18px 0 16px 0;
  position: relative; }

.stayPay .item_title_img {
  width: 76px;
  height: 76px;
  background-color: #7f4b2c;
  float: left; }
  .stayPay .item_title_img img {
    width: 100%;
    height: 100%; }

.stayPay .mainTitle {
  font-size: 12px;
  /*no*/
  color: #666;
  padding-left: 104px;
  padding-top: 10px; }
  [data-dpr="2"] .stayPay .mainTitle {
    font-size: 24px;
    /*no*/ }
  [data-dpr="3"] .stayPay .mainTitle {
    font-size: 36px;
    /*no*/ }

.stayPay .Subtitle {
  font-size: 10px;
  /*no*/
  color: #999;
  padding-left: 104px;
  padding-top: 5px; }
  [data-dpr="2"] .stayPay .Subtitle {
    font-size: 20px;
    /*no*/ }
  [data-dpr="3"] .stayPay .Subtitle {
    font-size: 30px;
    /*no*/ }

.stayPay .arrow {
  width: 13px;
  height: 22px;
  background-image: url("../images/orderList/arrow.png");
  background-size: cover;
  position: absolute;
  right: 0;
  top: 50%;
  -webkit-transform: translateY(-50%);
  -moz-transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  -o-transform: translateY(-50%);
  transform: translateY(-50%); }

.stayPay .item_number {
  padding: 20px 0 24px 0; }

.stayPay .item_number_left {
  float: left; }

.stayPay .item_font {
  font-size: 10px;
  /*no*/
  color: #666; }
  [data-dpr="2"] .stayPay .item_font {
    font-size: 20px;
    /*no*/ }
  [data-dpr="3"] .stayPay .item_font {
    font-size: 30px;
    /*no*/ }

.stayPay .item_font1 {
  margin-bottom: 15px; }
