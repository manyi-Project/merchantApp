.stayShipped .main {
  width: 720px;
  margin: 0 auto;
  padding-top: 100px; }

.stayShipped .shoppingList_box {
  padding: 0 20px;
  background-color: #ffffff;
  margin-top: 18px; }

.stayShipped .shoppingList_box li {
  border-bottom: 1px solid #e3e3e3;
  /*no*/ }
  [data-dpr="2"] .stayShipped .shoppingList_box li {
    border-bottom: 2px solid #e3e3e3;
    /*no*/ }
  [data-dpr="3"] .stayShipped .shoppingList_box li {
    border-bottom: 3px solid #e3e3e3;
    /*no*/ }

.stayShipped .shoppingList_box li:last-child {
  border-bottom: none;
  /*no*/ }

.stayShipped .item_title {
  /*height: 107px;*/
  padding: 18px 0 16px 0;
  position: relative; }

.stayShipped .item_title_img {
  width: 76px;
  height: 76px;
  background-color: #7f4b2c;
  float: left; }
  .stayShipped .item_title_img img {
    width: 100%;
    height: 100%; }

.stayShipped .mainTitle {
  font-size: 12px;
  /*no*/
  color: #666;
  padding-left: 104px;
  padding-top: 10px; }
  [data-dpr="2"] .stayShipped .mainTitle {
    font-size: 24px;
    /*no*/ }
  [data-dpr="3"] .stayShipped .mainTitle {
    font-size: 36px;
    /*no*/ }

.stayShipped .Subtitle {
  font-size: 10px;
  /*no*/
  color: #999;
  padding-left: 104px;
  padding-top: 5px; }
  [data-dpr="2"] .stayShipped .Subtitle {
    font-size: 20px;
    /*no*/ }
  [data-dpr="3"] .stayShipped .Subtitle {
    font-size: 30px;
    /*no*/ }

.stayShipped .arrow {
  width: 15px;
  height: 22px;
  background-image: url("../images/left-arrow.png");
  background-size: cover;
  position: absolute;
  right: 0;
  top: 50%;
  -webkit-transform: translateY(-50%);
  -moz-transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  -o-transform: translateY(-50%);
  transform: translateY(-50%); }

.stayShipped .item_number {
  padding: 20px 0 24px 0; }

.stayShipped .item_number_left {
  float: left; }

.stayShipped .item_font {
  font-size: 10px;
  /*no*/
  color: #666; }
  [data-dpr="2"] .stayShipped .item_font {
    font-size: 20px;
    /*no*/ }
  [data-dpr="3"] .stayShipped .item_font {
    font-size: 30px;
    /*no*/ }

.stayShipped .item_font1 {
  margin-bottom: 15px; }

.stayShipped .item_logistics {
  height: 60px;
  padding-right: 12px; }

.stayShipped .logistics1 {
  margin-right: 30px; }

.stayShipped .logistics_shipped {
  padding: 0 17px;
  height: 40px;
  border-radius: 5px;
  line-height: 40px;
  color: #FF8400;
  font-size: 10px;
  /*no*/
  text-align: center;
  border: 1px solid #ff8400;
  /*no*/
  -webkit-transform: translateY(25%);
  -moz-transform: translateY(25%);
  -ms-transform: translateY(25%);
  -o-transform: translateY(25%);
  transform: translateY(25%); }
  [data-dpr="2"] .stayShipped .logistics_shipped {
    font-size: 20px;
    /*no*/ }
  [data-dpr="3"] .stayShipped .logistics_shipped {
    font-size: 30px;
    /*no*/ }
  [data-dpr="2"] .stayShipped .logistics_shipped {
    border: 2px solid #ff8400;
    /*no*/ }
  [data-dpr="3"] .stayShipped .logistics_shipped {
    border: 3px solid #ff8400;
    /*no*/ }

.stayShipped .logistics {
  width: 110px;
  height: 40px;
  border-radius: 5px;
  /*no*/
  line-height: 40px;
  color: #FF8400;
  font-size: 10px;
  /*no*/
  text-align: center;
  border: 1px solid #ff8400;
  /*no*/
  float: right;
  -webkit-transform: translateY(25%);
  -moz-transform: translateY(25%);
  -ms-transform: translateY(25%);
  -o-transform: translateY(25%);
  transform: translateY(25%); }
  [data-dpr="2"] .stayShipped .logistics {
    font-size: 20px;
    /*no*/ }
  [data-dpr="3"] .stayShipped .logistics {
    font-size: 30px;
    /*no*/ }
  [data-dpr="2"] .stayShipped .logistics {
    border: 2px solid #ff8400;
    /*no*/ }
  [data-dpr="3"] .stayShipped .logistics {
    border: 3px solid #ff8400;
    /*no*/ }
